<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pwgame.passwordgame" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".gamePortal"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".levelActivity"
            android:label="@string/title_activity_level" >
        </activity>
        <activity
            android:name=".mainPage"
            android:label="@string/title_activity_main_page" >
        </activity>
        <activity
            android:name=".schemaLibrary"
            android:label="@string/title_activity_schema_library" >
        </activity>
        <activity
            android:name=".schemaBuilder"
            android:label="@string/title_activity_schema_builder" >
        </activity>
        <activity
            android:name=".postLevelActivity"
            android:label="@string/title_activity_post_level" >
        </activity>
        <activity
            android:name=".traceDisplayActivity"
            android:label="@string/title_activity_trace_display" >
        </activity>
        <activity
            android:name=".introduction"
            android:label="@string/title_activity_introduction" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".endGame"
            android:label="@string/title_activity_end_game" >
        </activity>
        <activity
            android:name=".source"
            android:label="@string/title_activity_source" >
        </activity>
        <activity
            android:name=".schemaBuilderMap"
            android:label="@string/title_activity_schema_builder_map" >
        </activity>
        <activity
            android:name=".schemaBuilderInstructions"
            android:label="@string/title_activity_schema_builder_instructions" >
        </activity>
        <activity
            android:name=".search"
            android:label="@string/title_activity_search" >
        </activity>
    </application>

</manifest>
